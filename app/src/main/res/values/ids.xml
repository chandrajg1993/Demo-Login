<?xml version="1.0" encoding="utf-8"?>
<resources>
    <item name="snackbar_text" type="id" />
    <declare-styleable name="SearchableSpinner">
        <attr name="hintText" format="string"/>
    </declare-styleable>
    <declare-styleable name="CircularView">
        <!--Radius of Outer Circle in dp-->
        <attr name="m_circle_radius" format="integer"/>

        <!--Stroke for Outer Circle in dp-->
        <attr name="m_cicle_stroke_width" format="integer"/>

        <!--Color of Stroke of Outer Circle-->
        <attr name="m_circle_stroke_color" format="color"/>

        <!--Color of rotating Arc-->
        <attr name="m_arc_stroke_color" format="color"/>

        <!--Arc margin from Circle in dp-->
        <attr name="m_arc_margin" format="integer"/>


    </declare-styleable>

    <!--<declare-styleable name="ds">
        <attr name="background_color" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="button_color" format="color"/>
    </declare-styleable>-->
</resources>